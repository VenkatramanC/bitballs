{{#player}}
	<p>{{name}}</p>
	<p>age: {{age}}</p>
	<p>weight: {{weight}}lbs</p>
	<p>height: {{height}}"</p>
{{/player}}

{{#each tournaments}}
	{{#if statsByTournament[id]}}
		<p>{{year}} Tournament</p>
		{{#each statsByTournament[id].aggregated}}
			<p>{{name}}: {{value}}</p>
		{{/each}}
	{{/if}}
{{/each}}
